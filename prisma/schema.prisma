// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model User {
  tgId     BigInt  @id
  isActive Boolean @default(true)
}

model Role {
  id   Int    @id @default(autoincrement())
  name String
}
